
  <view class="userinfo">
    <view class="userinfo-avatar">
      <open-data  type="userAvatarUrl"></open-data>
    </view>
  </view>
  <view class='username'>
    <open-data type="userNickName"></open-data>
  </view>
<van-cell-group>
  <van-cell
    title="用户积分"
    value="{{userIntegral}}"
    
    size=large
  />
  <van-cell
    title="用户类型"
    value="{{userType[0]}}"
    size=large
  />
  <van-cell
    bind:click="modifyPhone"
    clickable = true
    title="手机号码"
    value="{{userPhone}}"
    size=large
    is-link
    url="./userMsg/modifyPhone"
  />
  <van-cell
    title="我的订单"
    value="订单列表"
    size=large
    is-link
    url="./userOrders/userOrders"
  />
</van-cell-group>

